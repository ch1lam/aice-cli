{
  "name": "aice-cli",
  "description": "The AI Ace CLI.",
  "version": "0.0.1",
  "author": "chilam",
  "bin": {
    "aice": "./bin/run.js"
  },
  "bugs": "https://github.com/ch1lam/aice-cli/issues",
  "dependencies": {
    "@ai-sdk/deepseek": "^2.0.1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ai": "^6.0.3",
    "dotenv": "^17.2.3",
    "ink": "^6.6.0",
    "react": "^19.2.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/react": "^19.2.7",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "ink-testing-library": "^4.0.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/ch1lam/aice-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "aice",
    "dirname": "aice",
    "commands": {
      "strategy": "single",
      "target": "./dist/index.js"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "ch1lam/aice-cli",
  "scripts": {
    "dev": "DEBUG=* node bin/dev.js",
    "build": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "TSX_TSCONFIG_PATH=test/tsconfig.json node --import tsx ./node_modules/mocha/bin/mocha --forbid-only \"test/**/*.test.{ts,tsx}\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
